{
  "DOTNET_environment":  "development",
  "ConnectionStrings": {
    "Default": "MyConnectionString"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Trace"
      },
      "FileName": "syslog.txt",
      "Directory": "C:\\Users\\Usuario\\Documents\\GitHub\\NtCode",
      "EnsureDirectoryExists": "true",
      "LogAtTop": "false",
      "FormatterOptions": {
        "LogLevel": "true",
        "LogDateTime": "true",
        "Singleline": "false",
        "TimestampOptions": {
          "LogDateTime": "true",
          "LogDate": "true",
          "LogTime": "true",
          "LogMilliseconds": "false",
          "UseUtcTimestamp": "false",
          "Timestampformat": "yyyy/MM/dd HH:mm:ss.fff"
        }
      },
      "ColorConsole": {
        "LogLevelToColorMap": {
          "Information": "DarkGreen",
          "Warning": "Yellow",
          "Error": "Magenta"
        }
      }
    },
    "ColorConsole": {
      "LogLevelToColorMap": {
        "Information": "DarkGreen",
        "Warning": "Yellow",
        "Error": "Magenta"
      }
    }
  }
}